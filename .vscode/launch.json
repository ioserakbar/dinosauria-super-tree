{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Start API",
            "type": "node",
            "request": "launch",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/DinosauriaSuperTree.WebApi/src/server.ts"
        },
        {
            "name": "Start MVC",
            "type": "chrome",
            "request": "launch",
            "preLaunchTask": "npm: start - DinosauriaSuperTree.MVC",
            "url": "http://localhost:4200/"
        },
        {
            "name": "ng test",
            "type": "chrome",
            "request": "launch",
            "preLaunchTask": "npm: test",
            "url": "http://localhost:9876/debug.html"
        }
    ],
    "compounds": [
        {
            "name": "Run All",
            "configurations": ["Start API", "Start MVC"]
        }
    ]
}
